<%= form_for @submission, html: { multipart: true, class: "dropzone" } do |f| %>
  <div class="container">
    <div class="row">
      <div class="col-lg-12">
        <h1>New Submission</h1>
      </div>
      <div class="col-lg-12">
        <%= f.submit 'Create Submission', class: 'btn btn-success' %>
      </div>
    </div>
  </div>
<% end %>

