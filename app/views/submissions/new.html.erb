<%= form_for @submission, html: { multipart: true, class: "dropzone" } do |f| %>
  <div class="container">
    <div class="row">
      <div class="col-lg-12">
        <h1>New Submission</h1>
      </div>
      <div class="col-lg-12">
        <%= render partial: "shared/dropzone", locals: { record: @submission, form: f } %>
        <%= f.submit %>
      </div>
    </div>
  </div>
<% end %>

